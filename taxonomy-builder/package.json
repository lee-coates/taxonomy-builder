{
  "name": "taxonomy-builder",
  "version": "1.0.0",
  "description": "HTML5 Drag and Drop Taxonomy Builder to assist in developing classification schemas",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./index.js",
    "ui": "browser-sync start --proxy=localhost:3000 --files='**/*.css, **/*.pug, **/*.js' --ignore=node_modules --reload-delay 10 --no-ui --no-notify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lee-coates/taxonomy-builder.git"
  },
  "author": "lee-coates",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lee-coates/taxonomy-builder/issues"
  },
  "homepage": "https://github.com/lee-coates/taxonomy-builder#readme",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "3.0.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "nodemon": "^2.0.4"
  }
}
